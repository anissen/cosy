fn f() {
    print 'before return'
    if (false) return _
    print 'after first return'
    return _
    print 'after second return'
}

f()

for true {
    print 'before break'
    break
    print 'after break'
}

for 0..3 {
    print 'before continue'
    continue
    print 'after continue'
}

if 5 < 4 {
    print 'always false'
} else if 4 < 5 {
    print 'always true'
} else {
    print 'never hit'
}