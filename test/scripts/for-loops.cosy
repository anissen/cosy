for 0..2 {
    print 'loop'
}


for j in 0..2 {
    print 'loop #{j}'
}
let from = 2
let to = 4
for j in from..to {
    print 'loop #{j}'
}


mut i = 0
for i < 5 {
    print 'conditional loop #{i}'
    i = i + 1
    if (i >= 3) break
}


mut j = 0
for {
    print 'infinite loop #{j}'
    j = j + 1
    if (j >= 3) break
}
